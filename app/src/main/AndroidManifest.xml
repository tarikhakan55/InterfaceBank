        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main_interface.Main_interface" />
        <activity android:name=".User_activity" />
        <activity android:name=".user_interface.User_interface" />
        <activity android:name=".card_interface.Card_interface" />
        <activity android:name=".widget_interface.Widget_interface"></activity>
    </application>

</manifest>